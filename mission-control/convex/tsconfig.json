{
    "compilerOptions": {
        "target": "ES2021",
        "lib": [
            "ES2021"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "**/*"
    ],
    "exclude": [
        "node_modules",
        "_generated"
    ]
}